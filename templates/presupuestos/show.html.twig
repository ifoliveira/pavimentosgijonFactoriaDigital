{% extends 'inicio2.html.twig' %}
{% block eltiempo %} {% endblock %}
{% form_theme form 'fields.html.twig' %}
{% block cabecera %} Presupuestos {% endblock %}
{% block breadcrumb %} <li class="breadcrumb-item active">Presupuestos List</li>{% endblock %}
{% block title %}Presupuestos List{% endblock %}
{% block cardbody1 %}
                <!-- ============================================================== -->
                <!-- Start Page Content -->
                <!-- ============================================================== -->
                <!-- basic table -->
                <div class="row">
                    <div class="col-lg-8">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title" style="text-align:center">Ticket Info - {{ presupuesto.EstadoPe }}  
                                <h6>{{ presupuesto.clientePe.nombreCl }} {{ presupuesto.clientePe.apellidosCl }} </h6>
                                <p><b>Dirección : </b>{{ presupuesto.clientePe.direccionCl }} <b> &nbsp;  &nbsp; &nbsp;  Teléfono : </b> {{ presupuesto.clientePe.telefono1Cl }}  </p>
                                
                            </div>
                        </div>                    
            

                        {% if presupuesto.EstadoPe.id > 3 and presupuesto.EstadoPe.id < 8 %}
                                    <div class="card">
                                        <div class="card-body">
                                            {{ form_start(formestado) }}
                                                    {{ form_row(formestado._token) }}
                                                    {% if presupuesto.EstadoPe.id == 4 %}
                                                    <h4 class="card-title">¿Ya se ha medido e incorporado fotos?</h4>
                                                    <button type="submit" class="btn btn-rounded btn-outline-success">Medir finalizado </button>
                                                    {% endif %} 
                                                    {% if presupuesto.EstadoPe.id == 5 %}
                                                    <h4 class="card-title">¿Se han escogido los materiales?</h4>
                                                    <button type="submit" class="btn btn-rounded btn-outline-success">Escoger finalizado </button>
                                                    {% endif %} 
                                                    {% if presupuesto.EstadoPe.id == 6 %}
                                                    <h4 class="card-title">¿Presupuesto finalizado?</h4>
                                                    <button type="submit" class="btn btn-rounded btn-outline-success">Presupuesto finalizado </button>
                                                    {% endif %} 
                                                    {% if presupuesto.EstadoPe.id == 7 %}
                                                    <h4 class="card-title">¿Presupuesto Entregado?</h4>
                                                    <button type="submit" class="btn btn-rounded btn-outline-success">Entregar presupuesto</button>
                                                    
                                                    {% endif %} 
                                                    {{ form_row(formestado.manoobraPe | raw, { 'row_attr': {'class': 'd-none'} })  }}
                                                    {{ form_row(formestado.importetotPe, { 'row_attr': {'class': 'd-none'} }) }}
                                                    {{ form_row(formestado.importemanoobra, { 'row_attr': {'class': 'd-none'} }) }}
                                                    {{ form_row(formestado.descuaetoPe, { 'row_attr': {'class': 'd-none'} }) }}
                                                    {{ form_row(formestado.clientePe, { 'row_attr': {'class': 'd-none'} }) }}
                                            {{ form_end(formestado, {'render_rest': false}) }}

                                        </div>
                                    </div>

                        {% endif %}

                        {% if presupuesto.EstadoPe.id < 7 %}
                                    <div class="card">
                                        <div class="card-body">
                                             
                                        </div>
                                    </div>
                        {% endif %}

                        {% if presupuesto.EstadoPe.id == 6 or presupuesto.EstadoPe.id == 7 %}
                                    <div class="card">
                                        <div class="card-body">
                                             <a href="{{ path('presupuestos_generar', {id: presupuesto.id, precios: 'S', tipo : 'PRESUPUESTO'}) }}" target="_blank"  class="btn btn-rounded btn-outline-danger "> Presupuesto CON precios</a>
                                             <a href="{{ path('presupuestos_generar', {id: presupuesto.id, precios: 'N', tipo : 'PRESUPUESTO'}) }}" target="_blank"  class="btn btn-rounded btn-outline-danger "> Presupuesto SIN precios</a>
                                        </div>
                                    </div>
                        {% endif %}
                        {% if presupuesto.EstadoPe.id == 8 %}

                                    <div class="card">
                                        <div class="card-body">
                                            <button id="Aceptar" class="btn btn-rounded btn-outline-success " data-importe="{{presupuesto.importetotPe}}">Aceptar</button>
                                            <button id="Rechazar" class="btn btn-rounded btn-outline-danger " data-importe="{{presupuesto.importetotPe}}">Rechazar</button>
                                            <a href="{{ path('presupuestos_modificar', {id: presupuesto.id}) }}"  class="btn btn-rounded btn-outline-success "> Modificar</a>                                            
                                            <a href="{{ path('presupuestos_modificar_manoobra', {id: presupuesto.id}) }}"  class="btn btn-rounded btn-outline-success "> Modificar Mano Obra</a>                                                            
                                        </div>
                                    </div>

                                    <div class="card">
                                        <div class="card-body">
                                             <a href="{{ path('presupuestos_generar', {id: presupuesto.id, precios: 'S', tipo : 'PRESUPUESTO'}) }}" target="_blank"  class="btn btn-rounded btn-outline-danger "> Presupuesto CON precios</a>
                                             <a href="{{ path('presupuestos_generar', {id: presupuesto.id, precios: 'N', tipo : 'PRESUPUESTO'}) }}" target="_blank"  class="btn btn-rounded btn-outline-danger "> Presupuesto SIN precios</a>
                                        </div>
                                    </div>
                        {% endif %}

                        {% if presupuesto.EstadoPe.id == 9 %}

                                    <div class="card">
                                        <div class="card-body">
                                            <button id="Finalizar" class="btn btn-rounded btn-outline-success " data-importe="{{presupuesto.importetotPe}}">Finalizar</button>
                                            <a href="{{ path('presupuestos_modificar', {id: presupuesto.id}) }}"  class="btn btn-rounded btn-outline-success "> Modificar</a>                                            
                                            <a href="{{ path('presupuestos_modificar_manoobra', {id: presupuesto.id}) }}"  class="btn btn-rounded btn-outline-success "> Modificar Mano Obra</a>                                             
                                        </div>
                                    </div>
                                    <div class="card">
                                        <div class="card-body">
                                             <a href="{{ path('presupuestos_generar', {id: presupuesto.id, precios: 'S', tipo : 'FACTURA SIMPLIFICADA'}) }}" target="_blank"  class="btn btn-rounded btn-outline-danger "> Factura CON precios</a>
                                             <a href="{{ path('presupuestos_generar', {id: presupuesto.id, precios: 'N', tipo : 'FACTURA SIMPLIFICADA'}) }}" target="_blank"  class="btn btn-rounded btn-outline-danger "> Factura SIN precios</a>
                                             <a href="{{ path('presupuestos_generar', {id: presupuesto.id, precios: 'S', tipo : 'PRESUPUESTO'}) }}" target="_blank"  class="btn btn-rounded btn-outline-danger "> Presupuesto CON precios</a>
                                             <a href="{{ path('presupuestos_generar', {id: presupuesto.id, precios: 'N', tipo : 'PRESUPUESTO'}) }}" target="_blank"  class="btn btn-rounded btn-outline-danger "> Presupuesto SIN precios</a>
                                        </div>
                                    </div>
                        {% endif %}

                        {% if presupuesto.EstadoPe.id == 10 %}

                                    <div class="card">
                                        <div class="card-body">
                                            <a href="{{ path('presupuestos_generar', {id: presupuesto.id, precios: 'N', tipo : 'FACTURA FINAL'}) }}" target="_blank"  class="btn btn-rounded btn-outline-danger "> Factura SIN precios</a>
                                             <a href="{{ path('presupuestos_generar', {id: presupuesto.id, precios: 'S', tipo : 'FACTURA FINAL'}) }}" target="_blank"  class="btn btn-rounded btn-outline-danger "> Factura CON precios</a>
                                        </div>
                                    </div>
                        {% endif %}

                        {% if presupuesto.EstadoPe.id == 6 %}
                         <div class="card">
                            <div class="card-body">
  

                                <h3 class="card-title">PRODUCTOS</h3>
                                <h6 class="card-subtitle"></h6>
                                <div class="table-responsive">
                                    <table id="tbprod" class="table table-striped table-bordered text-inputs-searching">
                                         <thead>
                                             <tr>
                                                 <th>Id</th>
                                                 <th>tipo</th>        
                                                 <th>Descripcion</th>
                                                 <th>Precio</th>
                                                 <th>PVP</th>
                                                 <th>Stock</th>
                                             </tr>
                                         </thead>
                                         <tbody>
                                             {% for producto in productos %}
                                                 <tr>
                                                     <td>{{ producto.id }}</td>
                                                     <td>{{ producto.tipoPdId}}</td>
                                                     <td>{{ producto.descripcionPd }}</td>
                                                     <td>{{ producto.precioPd }}</td>
                                                     <td>{{ producto.pvpPd }}</td>
                                                     <td>{{ producto.stockPd }}</td>
                                                 </tr>
                                             {% else %}
                                                 <tr>
                                                     <td colspan="8">no records found</td>
                                                 </tr>
                                             {% endfor %}
                                         </tbody>
                                         <tfoot>
                                             <tr>
                                                 <th>Id</th>
                                                 <th>tipo</th>        
                                                 <th>Descripcion</th>
                                                 <th>Precio</th>
                                                 <th>PVP</th>
                                                 <th>Stock</th>
                                             </tr>
                                         </tfoot>
                                    </table>
                                </div>
                            </div>
                        </div>
                        {% endif %}


                       {% if presupuesto.EstadoPe.id == 7  or presupuesto.EstadoPe.id == 6 %}

                                    <div class="card">

                                        {{ form_start(formmanoob) }}
                                                {{ form_row(formmanoob._token) }}


                                            <div class="card-body">
                                            <h6 class="card-subtitle"></code></h6>
                                            <div class="action-form">
                                                <div class="form-group mb-0 text-left">
                                                    <button type="submit"  class="btn btn-info waves-effect waves-light">{{ button_label|default('Actualizar PVP') }}</button>
                                                </div>
                                            </div>
                                            <hr>
                                                {{ form_row(formmanoob.estadoPe, { 'row_attr': {'class': 'd-none'} }) }}
                                                {{ form_row(formmanoob.manoobraPe, { 'row_attr': {'class': 'd-none'} }) }}
                                                {{ form_row(formmanoob.importetotPe, { 'row_attr': {'class': 'd-none'} }) }}
                                                {{ form_row(formmanoob.clientePe, { 'row_attr': {'class': 'd-none'} }) }}
                                                {{ form_row(formmanoob.importemanoobra) }}
                                                {{ form_row(formmanoob.descuaetoPe, { 'row_attr': {'class': 'd-none'} }) }}
                                        {{ form_end(formmanoob, {'render_rest': false}) }}

                                        </div>
                                    </div>
                       {% endif %}



                        
                        {% if presupuesto.EstadoPe.id == 4 %}   
                            <div class="card">
                                <div class="card-body">
                                    <h4 class="card-title">Subir fotos</h4>
                                    {{ form_start(form) }}
                                        
                                            <div class="card-body">
                                            <h6 class="card-subtitle"></code></h6>
                                            <div class="action-form">
                                                <div class="form-group text-left">
                                                    <button type="submit"  class="btn btn-info waves-effect waves-light">{{ button_label|default('Guardar Foto') }}</button>
                                                </div>
                                            </div>
                                            <hr>
                                    
                                    {{ form_end(form) }}
                                    </div>
                                </div>
                            </div>
                        {% endif %}  



                    {% if presupuesto.EstadoPe.id > 8 %} 
                        <div class="card">
                            <div class="card-body">
                            <h4 class="card-title">Cobros pendientes</h4>
                                <h6 class="card-subtitle"></h6>
                                <div class="table-responsive">
                                    <table id="default_order" class="table table-striped table-bordered display"
                                        style="width:100%">
                                            <thead>
                                                <tr>
                                                    <th>Id</th>
                                                    <th>Concepto</th>
                                                    <th>Importe Total</th>
                                                    <th>Pendiente </th>
                                                    <th>Acciones</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    {% set pagado = 0 %}
                                                    <td>{{ presupuesto.ticket.id }}</td>
                                                    <td>Materiales</td>
                                                    <td align="right">{{ presupuesto.ticket.importeTotCs|format_currency('EUR', locale='es')  }}</td>
                                                    {% for pagos in presupuesto.ticket.pagos %}
                                                        {% set pagado = pagos.importePg + pagado %}
                                                    {% endfor %}
                                                    <td align="right">{{ (presupuesto.ticket.importeTotCs - pagado) |format_currency('EUR', locale='es')   }}</td>
                                                    <td>

                                                       {% if (presupuesto.ticket.importeTotCs - pagado) == 0 %}
                                                            <h5> No hay nada pendiente </h5>
                                                       {% else %}
                                                        <a class="text-inverse pr-2" data-toggle="tooltip" title="Añadir Pago" name="cerrar_senal" id="{{ presupuesto.ticket.id }}"   data-importe="{{ (presupuesto.ticket.importeTotCs - pagado)}}" data-url="{{ path("cestas_finalizar", {'id': presupuesto.ticket.id}) }}">
                                                            <i class="ti-money"></i>                                                        
                                                        </a>     
                                                        {% endif %}                                               
                                                    </td>
                                   

                                                </tr>

                                                
                                                    {% set manoobrapdte = 0 %}
                                                    {% set manoobratotal = 0 %}
                                                    {% set idmanoobra = 0 %}
                                                    {% for economicos in economic %}                                                
                                                        {% if economicos.aplicaEco == 'M' %}
                                                            {% set manoobratotal = manoobratotal + economicos.importeEco%}
                                                            {% if economicos.estadoEco == '1'%}
                                                                {% set idmanoobra = economicos.id %}
                                                                {% set manoobrapdte = economicos.importeEco %}
                                                            {% endif %}
                                                        {% endif %}
                                                    {% endfor %}
                                                <tr>

                                                            <td>{{ idmanoobra }}</td>
                                                            <td>Mano de Obra</td>
                                                            <td align="right">{{ manoobratotal|format_currency('EUR', locale='es')  }}</td>
                                                            <td align="right">{{ manoobrapdte|format_currency('EUR', locale='es')  }}</td>
                                                            <td>
                                                            {% if manoobrapdte == 0 %}
                                                                <h5> No hay nada pendiente </h5>
                                                            {% else %}                                                            
                                                               <a class="text-inverse pr-2" data-toggle="tooltip" title="Tratar movimiento" name="ticket_mano" id="{{ idmanoobra }}" data-importe="{{ manoobrapdte}}" ><i class="ti-money"></i></a>                                                        
                                                            {% endif %}
                                                            </td>                                                
                                                </tr>

                                                <tr>
                                                    <td></td>
                                                    <td>Total pendiente</td>
                                                    <td align="right"></td>
                                                    <td align="right">{{ (presupuesto.ticket.importeTotCs - pagado + manoobrapdte)|format_currency('EUR', locale='es')  }}</td>
                                                    <td></td>
                                                </tr>                                                


                                            </tbody>
                                        </table>
                                </div>
                            </div>                        
                            <div class="card-body">
                            <h4 class="card-title">Movimientos Mano de Obra</h4>
                                <h6 class="card-subtitle"></h6>
                                <div class="table-responsive">
                                    <table id="default_order" class="table table-striped table-bordered display"
                                        style="width:100%">
                                            <thead>
                                                <tr>
                                                    <th>Id</th>
                                                    <th>Concepto</th>
                                                    <th>Importe Total</th>
                                                    <th>Estado</th>
                                                    <th>Acciones</th>
                                                </tr>
                                            </thead>
                                             {% set importeapagar = 0 %}
                                            <tbody>
                                             {% for economicos in economic %}
                                                {% if  economicos.aplicaEco != 'M' %}
                                                    {% if economicos.estadoEco != 9  %}
                                                        <tr>
                                                            <td>{{ economicos.id }}</td>
                                                            <td>{{ economicos.conceptoEco}}</td>
                                                            {% if economicos.debehaberEco == 'D'  %}
                                                                <td align="right">{{ (economicos.importeEco * -1)|format_currency('EUR', locale='es')}}</td>
                                                                {% set importeapagar = (economicos.importeEco * -1) + importeapagar %}
                                                            {% else %}
                                                                <td align="right">{{ economicos.importeEco|format_currency('EUR', locale='es')}}</td>
                                                                {% set importeapagar = (economicos.importeEco) + importeapagar %}
                                                            {% endif %}

                                                            {% if economicos.estadoEco == '1' %}
                                                                <td>Pendiente</td>
                                                                <td>
                                                                        {% if economicos.aplicaEco != 'M' %}

                                                                            <a class="text-inverse pr-2" data-toggle="tooltip" title="Tratar movimiento" name="cambiar_importe" id="{{ economicos.id }}" data-importe="{{ economicos.importeEco}}"><i class="ti-money"></i></a> 
                                                                            
                                                                            {% if economicos.conceptoEco != 'Otros' and economicos.importeEco == 0 %}                                                       
                                                                                <a class="text-inverse pr-2" data-toggle="tooltip" title="Cambiar a no aplica" name="no_aplica" id="{{ economicos.id }}" data-estado= "9"><i class="ti-thumb-down"></i></a>
                                                                            {% endif %}
                                                                        {% endif %}    

                                                                </td>    
                                                            {% elseif economicos.estadoEco == '9' %}
                                                                <td>No aplica</td>
                                                                <td><a class="text-inverse pr-2" data-toggle="tooltip" title="Cambiar a aplica" name="no_aplica" id="{{ economicos.id }}" data-estado= "1"><i class="ti-thumb-up"></a></td>                                                                                                        
                                                            {% elseif economicos.estadoEco == '8' %}
                                                                <td>Pagado</td>
                                                                <td></td>
                                                            {% elseif economicos.estadoEco == '7' %}
                                                                <td>Cobrado con tarjeta</td>
                                                                <td></td>                                                        
                                                            {% elseif economicos.estadoEco == '6' %}
                                                                <td>Cobrado</td>
                                                                <td></td>                                                        
                                                            {% else %}
                                                                <td>{{ economicos.estadoEco}}</td>
                                                                <td></td>
                                                            {% endif %}

                                                    
                                                        
                                                        </tr>
                                                    {% endif %}
                                                {% endif %}
                                            {% endfor %}
                                            </tbody>
                                            <tfoot>
                                                <td>Total mano de obra</td>
                                                <td>{{ manoobratotal|format_currency('EUR', locale='es')  }}</td>
                                                <td>{{ importeapagar |format_currency('EUR', locale='es')}}</td>
                                                <td>{{(manoobratotal + importeapagar) |format_currency('EUR', locale='es')}}</td>
                                            </tfoot>
                                        </table>
                                </div>   
                            </div>
                        </div>
                        {% endif %}
                        {% if presupuesto.EstadoPe.id > 6 %} 
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">Presupuesto Actual</h4>
                                <h6 class="card-subtitle"></h6>
                                <div class="table-responsive">
                                    <table id="footer" class="table table-striped table-bordered display"
                                        style="width:100%">
                                        <thead>
                                            <tr align="center"> 
                                                <th>Detalle</th>
                                                <th>Precio</th>
                                                <th>Unidades</th>
                                                <th>PVP</th>
                                                <th>Coste</th>
                                                <th>Beneficio</th>
                                                <th>%</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        {% for detalle in project.getCesta(cestaId).getdetallecesta %}
                                            <tr>
                                                <td>{{ detalle.productoDc.descripcionPd }}</td>
                                                <td align="right">{{ detalle.pvpDc |format_currency('EUR', locale='es') }}</td>
                                                <td align="center">{{ detalle.cantidadDC}}</td>
                                                <td align="right">{{ (detalle.cantidadDC * detalle.pvpDc)|format_currency('EUR', locale='es') }}</td>
                                                <td align="right">{{ (detalle.precioDC * detalle.cantidadDc)|format_currency('EUR', locale='es') }}</td>
                                                <td align="right">{{ ((detalle.cantidadDC * detalle.pvpDc) - (detalle.precioDC * detalle.cantidadDc))|format_currency('EUR', locale='es') }}</td>
                                                <td align="right">{{ (((detalle.cantidadDC * detalle.pvpDc) - (detalle.precioDC * detalle.cantidadDc))  / (detalle.pvpDc * detalle.cantidadDc))|format_percent_number }}</td>
                                            </tr>
                                        {% else %}
                                            <tr>
                                                <td colspan="7">no records found</td>
                                            </tr>
                                        {% endfor %}
                                        </tbody>
                                        <tfoot>
                                            <tr>
                                                <th colspan="3" style="text-align:right">Total:</th>
                                                <th style="text-align:right">{{project.getImporteTot(cestaId)|format_currency('EUR', locale='es')  }}</th>
                                                <th style="text-align:right">{{project.getPrecioTot(cestaId)|format_currency('EUR', locale='es')  }}</th>
                                                <th style="text-align:right">{{(project.getImporteTot(cestaId) - project.getPrecioTot(cestaId))|format_currency('EUR', locale='es')  }}</th>
                                                <th style="text-align:right">{{(((project.getImporteTot(cestaId) - project.getPrecioTot(cestaId)) ) / (project.getImporteTot(cestaId)))|format_percent_number  }}</th>
                                            </tr>
                                        </tfoot>
                                    </table>
                                   
                                </div>
                            </div>
                        </div>
                    {% endif %} 
                    {% if presupuesto.EstadoPe.id > 6 and  presupuesto.EstadoPe.id < 8 %} 
                        <div class="card">
                            <div class="card-body">
  

                                <h3 class="card-title">PRODUCTOS</h3>
                                <h6 class="card-subtitle"></h6>
                                <div class="table-responsive">
                                    <table id="tbprod" class="table table-striped table-bordered text-inputs-searching">
                                         <thead>
                                             <tr>
                                                 <th>Id</th>
                                                 <th>tipo</th>        
                                                 <th>Descripcion</th>
                                                 <th>Precio</th>
                                                 <th>PVP</th>
                                                 <th>Stock</th>
                                             </tr>
                                         </thead>
                                         <tbody>
                                             {% for producto in productos %}
                                                 <tr>
                                                     <td>{{ producto.id }}</td>
                                                     <td>{{ producto.tipoPdId}}</td>
                                                     <td>{{ producto.descripcionPd }}</td>
                                                     <td>{{ producto.precioPd }}</td>
                                                     <td>{{ producto.pvpPd }}</td>
                                                     <td>{{ producto.stockPd }}</td>
                                                 </tr>
                                             {% else %}
                                                 <tr>
                                                     <td colspan="8">no records found</td>
                                                 </tr>
                                             {% endfor %}
                                         </tbody>
                                         <tfoot>
                                             <tr>
                                                 <th>Id</th>
                                                 <th>tipo</th>        
                                                 <th>Descripcion</th>
                                                 <th>Precio</th>
                                                 <th>PVP</th>
                                                 <th>Stock</th>
                                             </tr>
                                         </tfoot>
                                    </table>
                                </div>
                            </div>
                        </div>
                        

                        {% endif %}
                        {% set manoobratotal = 0%}
                        {% if presupuesto.EstadoPe.id < 9 %}
                        <div class="card">
                      
                            <div class="card-body">
                                <div class="action-form">
                                   <div class="form-group mb-0 text-left">
                                        <button  id="NuevaMano" type="submit"  class="btn btn-info waves-effect waves-light">{{ button_label|default('Nueva Mano de Obra') }}</button>
                                    </div>
                                </div>
                                <hr>                              
                                
                                <div class="table-responsive">
                                    <table id="footer" class="table table-striped table-bordered display"
                                        style="width:100%">
                                        <thead>
                                            <tr align="center"> 
                                                <th>Tipo</th>
                                                <th>Detalle</th>
                                                <th>Coste</th>
                                                <th>Acciones</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        {% for manodeobra in presupuesto.manoObra %}
                                            {% set manoobratotal = manodeobra.coste + manoobratotal %}
                                            <tr>
                                                <td>{{ manodeobra.categoriaMo.TipoTm}}</td>
                                                <td align="left">{{ manodeobra.textoMo}}</td>
